<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboard_navigation"
    app:startDestination="@id/dashboardFragment">

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.minibit.myapplication.ui.dashboard.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard">
        <action
            android:id="@+id/action_dashboardFragment_to_fooFragment"
            app:destination="@id/fooFragment" />
    </fragment>
    <fragment
        android:id="@+id/fooFragment"
        android:name="com.minibit.myapplication.ui.screen.FooFragment"
        android:label="FooFragment"
        tools:layout="@layout/fragment_foo" />
</navigation>